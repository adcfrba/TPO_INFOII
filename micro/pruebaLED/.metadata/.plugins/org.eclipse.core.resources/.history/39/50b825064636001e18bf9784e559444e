#include <cr_section_macros.h>
#include 	"Defines.h"
#define PIN_TECLA_USR 0,4


int main(void) {

	uint32_t contador = 0;
	SYSCON->SYSAHBCLKCTRL0 |= (1 << 6) | (1 << 20);

    while(1) {
    	if(!GPIO_Read(PIN_TECLA_USR))
    	{
    		contador++;
    	}
    	GPIO_Set(PIN_TECLA_USR);
    }
    return 0 ;
}
